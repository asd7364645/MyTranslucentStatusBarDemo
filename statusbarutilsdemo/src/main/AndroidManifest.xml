<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.alexleo.statusbarutilsdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.alexleo.statusbarutilsdemo.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.alexleo.statusbarutilsdemo.ColorStatusBarActivity"
            android:label="@string/app_name" />
        <activity android:name="com.alexleo.statusbarutilsdemo.ImageStatusBarActivity" />
        <activity android:name="com.alexleo.statusbarutilsdemo.ImageViewActivity" />
        <activity android:name="com.alexleo.statusbarutilsdemo.UseInFragmentActivity" />
        <activity android:name="com.alexleo.statusbarutilsdemo.SwipeBackActivity" />
    </application>

</manifest>